{
  "name": "linkshrink",
  "version": "1.0.0",
  "description": "URL shortener",
  "main": "app.js",
  "scripts": {
    "remove-assets": "ts-node tools/removeAssets",
    "copy-assets": "ts-node tools/copyAssets",
    "build": "npm-run-all remove-assets copy-assets",
    "dev:start": "npm-run-all build start-node",
    "start": "tsc-watch --onSuccess \"npm run dev:start\"",
    "start-node": "node dist/app.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanovskyi/linkShrink.git"
  },
  "keywords": [
    "link",
    "shrink",
    "url",
    "short",
    "shortener"
  ],
  "author": "Pavlo Yanovskyi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yanovskyi/linkShrink/issues"
  },
  "homepage": "https://github.com/yanovskyi/linkShrink#readme",
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.7",
    "@types/shelljs": "^0.8.12",
    "fs-extra": "11.1.1",
    "jest": "29.6.4",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.1",
    "shelljs": "0.8.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.4",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "ejs": "3.1.9",
    "express": "4.18.2",
    "pg": "8.11.3",
    "pg-hstore": "2.3.4",
    "sequelize": "6.32.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
